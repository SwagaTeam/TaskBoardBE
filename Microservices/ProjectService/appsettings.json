{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "NotificationTask": {
      "BootstrapServers": "localhost:29092",
      "Topic": "new-task",
      "GroupId": "task-created"
    },
    "CommentTask": {
      "BootstrapServers": "localhost:29092",
      "Topic": "new-comment",
      "GroupId": "comment-created"
    },
    "InviteToProject": {
      "BootstrapServers": "localhost:29092",
      "Topic": "new-invite",
      "GroupId": "invite-created"
    }
  }
}
